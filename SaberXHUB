local SupportedPlaceIds = {
    [3823781113] = "Map A"
}

local placeId = game.PlaceId

if SupportedPlaceIds[placeId] then
    print("✅ รองรับแมพ: "..SupportedPlaceIds[placeId])
    loadstring(game:HttpGet(
        "https://raw.githubusercontent.com/khaotosurasak-droid/HUB/refs/heads/main/SaberXHUB",
        true
    ))()
else
    warn("❌ แมพนี้ไม่รองรับสคริปต์")
end
