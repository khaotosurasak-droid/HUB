local Players = game:GetService("Players")
local player = Players.LocalPlayer

-- ‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô‡∏™‡∏£‡πâ‡∏≤‡∏á GUI
local function createGui()
    -- ‡∏•‡∏ö‡∏Ç‡∏≠‡∏á‡πÄ‡∏Å‡πà‡∏≤‡∏≠‡∏≠‡∏Å‡∏ñ‡πâ‡∏≤‡∏°‡∏µ
    if player.PlayerGui:FindFirstChild("WalkSpeedGui") then
        player.PlayerGui.WalkSpeedGui:Destroy()
    end

    local WalkSpeedGui = Instance.new("ScreenGui")
    WalkSpeedGui.Name = "WalkSpeedGui"
    WalkSpeedGui.Parent = player:WaitForChild("PlayerGui")

    -- Frame ‡∏´‡∏•‡∏±‡∏Å
    local Frame = Instance.new("Frame")
    Frame.Parent = WalkSpeedGui
    Frame.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
    Frame.Position = UDim2.new(0, 200, 0, 30)
    Frame.Size = UDim2.new(0, 200, 0, 200)
    Frame.Active = true
    Frame.ClipsDescendants = false

    local FrameCorner = Instance.new("UICorner")
    FrameCorner.CornerRadius = UDim.new(0, 12)
    FrameCorner.Parent = Frame

    -- TextLabel
    local TextLabel = Instance.new("TextLabel")
    TextLabel.Parent = Frame
    TextLabel.Size = UDim2.new(0, 90, 0, 20)
    TextLabel.Position = UDim2.new(0, 5, 0, 5)
    TextLabel.BackgroundColor3 = Color3.fromRGB(240, 240, 240)
    TextLabel.Text = "‡πÑ‡∏ó‡∏¢‡πÇ‡∏ß‡πâ‡∏¢üáπüá≠"
    TextLabel.TextColor3 = Color3.fromRGB(27, 42, 53)
    TextLabel.TextSize = 14

    local TextLabelCorner = Instance.new("UICorner")
    TextLabelCorner.CornerRadius = UDim.new(0, 8)
    TextLabelCorner.Parent = TextLabel

    -- ‡∏õ‡∏∏‡πà‡∏°‡∏ß‡∏¥‡πà‡∏á‡πÄ‡∏£‡πá‡∏ß
    local TextButton = Instance.new("TextButton")
    TextButton.Parent = Frame
    TextButton.Size = UDim2.new(0, 100, 0, 50)
    TextButton.Position = UDim2.new(0, 50, 0, 50)
    TextButton.Text = "‡∏ß‡∏¥‡πà‡∏á‡πÄ‡∏£‡πá‡∏ß"
    TextButton.BackgroundColor3 = Color3.fromRGB(255, 0, 0)
    TextButton.TextColor3 = Color3.fromRGB(255, 255, 255)
    TextButton.TextSize = 15

    local ButtonCorner = Instance.new("UICorner")
    ButtonCorner.CornerRadius = UDim.new(0, 12)
    ButtonCorner.Parent = TextButton

    local ButtonStroke = Instance.new("UIStroke")
    ButtonStroke.Parent = TextButton
    ButtonStroke.Thickness = 2
    ButtonStroke.Color = Color3.fromRGB(0, 0, 0)

    local isFast = false
    TextButton.MouseButton1Click:Connect(function()
        local character = player.Character or player.CharacterAdded:Wait()
        local humanoid = character:FindFirstChildOfClass("Humanoid")
        if humanoid then
            if isFast then
                humanoid.WalkSpeed = 16
                TextButton.Text = "‡∏ß‡∏¥‡πà‡∏á‡πÄ‡∏£‡πá‡∏ß"
            else
                humanoid.WalkSpeed = 50
                TextButton.Text = "‡πÄ‡∏î‡∏¥‡∏ô‡∏õ‡∏Å‡∏ï‡∏¥"
            end
            isFast = not isFast
        end
    end)

    -- ‡∏õ‡∏∏‡πà‡∏°‡∏¢‡πà‡∏≠
    local MinimizeButton = Instance.new("TextButton")
    MinimizeButton.Parent = Frame
    MinimizeButton.Size = UDim2.new(0, 20, 0, 20)
    MinimizeButton.Position = UDim2.new(1, -25, 0, 5)
    MinimizeButton.Text = "-"
    MinimizeButton.BackgroundColor3 = Color3.fromRGB(200, 0, 0)
    local MinimizeCorner = Instance.new("UICorner")
    MinimizeCorner.CornerRadius = UDim.new(0, 8)
    MinimizeCorner.Parent = MinimizeButton

    -- ‡∏õ‡∏∏‡πà‡∏°‡πÄ‡∏õ‡∏¥‡∏î‡πÉ‡∏´‡∏°‡πà
    local OpenButton = Instance.new("TextButton")
    OpenButton.Parent = WalkSpeedGui
    OpenButton.Size = UDim2.new(0, 100, 0, 30)
    OpenButton.Position = UDim2.new(0, 200, 0, 250)
    OpenButton.Text = "‡πÄ‡∏õ‡∏¥‡∏î GUI"
    OpenButton.BackgroundColor3 = Color3.fromRGB(0, 200, 0)
    OpenButton.Visible = false
    local OpenCorner = Instance.new("UICorner")
    OpenCorner.CornerRadius = UDim.new(0, 12)
    OpenCorner.Parent = OpenButton

    -- ‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô‡∏õ‡∏∏‡πà‡∏°‡∏¢‡πà‡∏≠/‡πÄ‡∏õ‡∏¥‡∏î
    MinimizeButton.MouseButton1Click:Connect(function()
        Frame.Visible = false
        OpenButton.Visible = true
    end)

    OpenButton.MouseButton1Click:Connect(function()
        Frame.Visible = true
        OpenButton.Visible = false
    end)
end

-- ‡πÄ‡∏£‡∏µ‡∏¢‡∏Å‡∏ï‡∏≠‡∏ô‡πÅ‡∏£‡∏Å
createGui()

-- ‡πÄ‡∏£‡∏µ‡∏¢‡∏Å‡∏ó‡∏∏‡∏Å‡∏Ñ‡∏£‡∏±‡πâ‡∏á‡∏ó‡∏µ‡πà respawn
player.CharacterAdded:Connect(function()
    createGui()
end)
